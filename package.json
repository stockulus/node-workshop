{
  "name": "node-workshop",
  "version": "0.1.0",
  "description": "workshop app",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon | pino-pretty -t",
    "build": "npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "test": "cross-env npm run lint && npm run typescript",
    "lint": "eslint src/*",
    "lint:fix": "eslint . --fix",
    "typescript": "tsc -b",
    "updtr": "updtr --save-exact"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.*"
    ],
    "exec": "ts-node --project tsconfig.server.json src/index.ts"
  },
  "dependencies": {
    "fastify": "2.2.0"
  },
  "devDependencies": {
    "@types/node": "11.13.2",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "nodemon": "1.18.11",
    "pino-pretty": "2.6.0",
    "prettier": "1.16.4",
    "ts-node": "8.0.3",
    "typescript": "3.4.2",
    "updtr": "3.1.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stockulus/node-workshop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stockulus/node-workshop/issues"
  },
  "homepage": "https://github.com/stockulus/node-workshop#readme"
}
